<script>
  // เมื่อหน้าโดนโหลดจาก back/forward cache (user กดปุ่ม Back ของ browser)
  window.addEventListener('pageshow', function (event) {
    try {
      var navEntries = performance.getEntriesByType("navigation");
      var navType = navEntries && navEntries.length ? navEntries[0].type : null;

      if (event.persisted || navType === "back_forward") {
        // รีเซ็ตฟอร์มทั้งหมดบนหน้านี้
        document.querySelectorAll('form').forEach(function (form) {
          form.reset();
        });

        // ถ้ามี input พิเศษอื่น ๆ อยากเคลียร์เพิ่ม ก็ทำในนี้ได้
        // เช่น: ล้าง radio/checkbox manual (บาง browser ดื้อ)
        // document.querySelectorAll('input[type=radio], input[type=checkbox]').forEach(function (el) {
        //   el.checked = false;
        // });
      }
    } catch (e) {
      // เงียบไป ไม่ให้ user เห็น error JS
      console && console.warn && console.warn(e);
    }
  });


// ป้องกันการคัดลอกและคลิกขวา
  document.addEventListener('contextmenu', function(e) {
    e.preventDefault(); // ป้องกันการเปิดเมนูคลิกขวา
  });

  document.addEventListener('keydown', function(e) {
    // ป้องกัน Ctrl+C, Ctrl+X, Ctrl+A (select all)
    // NOTE: บางเบราว์เซอร์อาจใช้ Cmd แทน Ctrl บน Mac
    if ((e.ctrlKey || e.metaKey) && (e.key === 'c' || e.key === 'x' || e.key === 'a')) {
      e.preventDefault();
    }
  });

  document.addEventListener('copy', function(e) {
    e.preventDefault(); // ป้องกันการคัดลอกเมื่อมี Selection และกด Ctrl+C/Cmd+C
  });

  // ป้องกันการเลือกข้อความ (Selection)
  document.body.onselectstart = function() {
    return false;
  };

</script>



